{
	"env": {
		"browser": true,
		"es6": true,
		"mocha": true
	},
	"globals": {
		"console": false
	},
	"plugins": [
		"react"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"rules": {
		"comma-dangle": "off",
		"curly": "error",
		"eqeqeq": ["error", "allow-null"],
		"no-case-declarations": "off",
		"no-else-return": "error",
		"react/no-find-dom-node": "warn",
		"react/prop-types": "error",
	}
}
