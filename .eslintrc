{
	"extends": [
		"eslint:recommended"
	],
	"rules": {
		"brace-style": [
			2,
			"1tbs",
			{
				"allowSingleLine": true
			}
		],
		"camelcase": [
			2,
			{
				"properties": "always"
			}
		],
		"complexity": [
			2,
			10
		],
		"curly": [
			2,
			"all"
		],
		"eqeqeq": [
			2,
			"smart"
		],
		"no-extend-native": 2,
		"no-unused-vars": [
			2,
			{
				"vars": "all",
				"args": "none"
			}
		],
		"one-var": [
			2,
			"never"
		],
		"quotes": [
			2,
			"single"
		],
		"react/jsx-uses-react": 1,
		"react/jsx-uses-vars": 1
	},
	"env": {
		"browser": true,
		"es6": true
	},
	"ecmaFeatures": {
		"jsx": true,
		"modules": true
	},
	"parser": "babel-eslint",
	"plugins": [
		"react"
	]
}