{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true
	},
	"plugins": [
		"react"
	],
	"rules": {
		"brace-style": [2, "1tbs", {"allowSingleLine": true}],
		"camelcase": [2, {"properties": "always"}],
		"complexity": [2, 10],
		"curly": [2, "all"],
		"eqeqeq": [2, "smart"],
		"no-extend-native": 2,
		"no-unused-vars": [2, {"vars": "all", "args": "none"}],
		"no-console": [1],
		"one-var": [2, "never"],
		"quotes": [2, "single"]
	}
}
