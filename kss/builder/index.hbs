<!DOCTYPE html>

<html class="no-js xui-html" lang="en">

<head>
<title>{{options.title}} {{#unless homepage}} â€“ {{sections.0.header}}{{/unless}} </title>
<meta charset="utf-8">
<meta name="generator" content="kss-node">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
{{{styles}}}
</head>

<header class="ds-nav--responsive ds-nav--header xui-padding xui-text-inverted">
	<a href="#navigation-primary" class="ds-hamburger"></a>
	<span class="ds-nav--header-title xui-padding-small">{{options.title}}</span>
</header>

<body id="kss-node" class="ds-node xui-body">


<article role="main" class="ds-main">

	<div class="ds-section-wrap">

		{{#if homepage}}
		<div id="home" class="kss-style ds-section-part">
			{{{homepage}}}
		</div>
		{{else}}

		{{!
		Display each section, in order.

		The "root" element comes first in this loop, and can be detected using
		the "#if @first" block as seen below.
		}}

		{{#each sections}}
		{{#wrapSection referenceURI}}

			<div class="kss-style ds-section-part" data-search-section>
				<h{{depth}} class="ds-title ds-title--level-{{depth}}" data-search-section-title {{#ifDepth 1}}data-search-page-title{{/ifDepth}}>
					<a class="kss-title__permalink ds-anchor" href="#{{referenceURI}}" data-search-section-link>
						<svg class="xui-icon xui-icon-inline"> <use xlink:href="#xui-icon-hash" role="presentation"/></svg>
					</a>
					{{header}}
				</h{{depth}}>

				{{#if description}}
				<div class="ds-description">
					{{{description}}}
				</div>
				{{/if}}

				{{#if parameters}}
				<div class="kss-parameters__title">Parameters:</div>
				<ul class="kss-parameters">
					{{#each parameters}}
					<li class="kss-parameters__item">
					<div class="kss-parameters__name"><code>{{name}}</code></div>
					<div class="kss-parameters__description">
						{{{description}}}
						{{#if defaultValue}}
						<div class="kss-parameters__default-value">
							Defaults to: <code>{{defaultValue}}</code>
						</div>
						{{/if}}
					</div>
					</li>
					{{/each}}
				</ul>
				{{/if}}
			</div>

			{{#if @first}} {{#each @root.menu}} {{#if isActive}} {{#if children}}
			<nav class="ds-section-part">
				<ul id="ds-nav-section" class="ds-nav ds-nav-section xui-tabgroup xui-tabgroup-vertical xui-tabgroup-vertical-layout">
					{{#each children}} {{#ifDepth 2}}
					<li>
					<a class="ds-nav-item xui-verticaltab" href="section-{{../referenceURI}}.html#{{referenceURI}}">
						<span>{{header}}</span>
					</a>
					</li>
					{{/ifDepth}} {{/each}}
				</ul>
			</nav>
			{{/if}} {{/if}} {{/each}} {{/if}}

			{{#if tokens}}
			<div class="kss-modifier__example ds-section-part">
				<div class="ds-token-wrap">
				<div class="ds-token-table xui-row-flex xui-justify-left">
					<div class="xui-column-12-of-12 xui-sectionheading xui-sectionheading-layout xui-text-sectionheading">
						<div class="xui-column-5-of-12">Token</div>
						<div class="xui-column-3-of-12">Value</div>
						{{#unless noexample}}<div class="xui-column-3-of-12">Example</div>{{/unless}}
					</div>
					{{#renderTokens tokens}}
					<div class="ds-token-table--row ds-token-vertical-align xui-column-12-of-12">
						<div class="xui-column-5-of-12"><code class="ds-token-text">{{token.name}}</code></div>
							<div {{#if token.example}}class="xui-column-3-of-12"{{/if}}>
									<code class="ds-token-text">{{token.value}}</code></div>
							{{#if token.example}}
							<div class="xui-column-4-of-12">{{{token.example}}}</div>
							{{/if}}
					</div>
					{{/renderTokens}}
				</div>
				</div>
			</div>
			{{/if}}

			{{#if colortokens}}
			<div class="kss-modifier__example ds-section-part">
				<div class="ds-token-wrap {{#isInverted header}}xui-text-inverted{{/isInverted}}">
				<div class="ds-token-table ds-token-table-color xui-row-flex xui-justify-left">
				{{#renderTokens colortokens}}
				<div class="ds-token-table--row xui-column-3-of-12">
					<div class="ds-token-grid--cell">
							<code class="ds-token-text">{{token.name}}</code>
					</div>
					<div class="ds-token-grid--cell">
						{{{token.example}}}
						<code class="ds-token-text--floating">{{token.value}}</code>
					</div>
				</div>
				{{/renderTokens}}
				</div>
				</div>
			</div>
			{{/if}}

			{{#if example}}
			<div class="ds-markup-example-switcher ds-section-part" data-sectionid="{{referenceNumber}}">
					<!-- These come first because they are used for styling siblings -->
					<input type="radio" id="markup-{{referenceNumber}}" name="a-{{referenceNumber}}" class="ds-input-markup" />
					<input checked type="radio" id="example-{{referenceNumber}}" name="a-{{referenceNumber}}" class="ds-input-example" />
					<input type="radio" id="all-{{referenceNumber}}" name="a-{{referenceNumber}}" class="ds-input-all" />

					<!-- hide markup switcher if it is only an example -->

					<div>
						<label for="markup-{{referenceNumber}}" class="ds-markup-label">Markup </label>
						<label for="example-{{referenceNumber}}" class="ds-example-label">Demo</label>
						<label for="all-{{referenceNumber}}" class="ds-all-label">Both</label>
					</div>


					<div class="ds-example-wrap">
						{{#if modifiers}}
						<div class="kss-modifier__default-name kss-style ds-section-part">
							Default styling
						</div>
						{{/if}}

						<div class="kss-modifier__example ds-section-part">
							{{{example}}}
						</div>

						{{#each modifiers}}
						<div class="kss-modifier__name kss-style ds-section-part">
							{{name}}
						</div>
						<div class="kss-modifier__description kss-style ds-section-part">
							{{{description}}}
						</div>
						<div class="kss-modifier__example ds-section-part">
							{{{markup}}}
						</div>
						{{/each}}
						<!--</div>-->

					</div> <!--ds example  wrap-->

					{{#if example}}
					<div class="ds-markup-wrap">
						<div class="kss-markup kss-style ds-section-part">
							<button data-clipboard-text="{{markup}}" class="ds-copy-button xui-button xui-button-standard xui-button-small">Copy</button>
							<pre class="prettyprint linenums lang-html"><code data-language="html">{{markup}}</code></pre>
					</div>

					{{#if sourceFile.name}}
					<div class="kss-source kss-style ds-section-part">
						Source: <a class="xui-text-minorlink" href="https://github.dev.xero.com/UXE/xui/blob/{{version}}/src/sass/{{sourceFile.name}}#L{{sourceFile.line}}">{{sourceFile.name}}</a></code>, line {{sourceFile.line}}
					</div>
					{{/if}}
							</div>
							{{/if}}

			</div>
			{{/if}}

		{{/wrapSection}}
		{{/each}}

		{{/if}}
	</div>
</article>

<nav id="navigation-primary" class="ds-nav ds-nav--menu">
	<ul class="xui-u-hidden ds-nav--container xui-tabgroup xui-tabgroup-vertical xui-tabgroup-vertical-layout">
    <li class="xui-menugroup xui-padding ds-nav--header xui-text-inverted">
			<img class="ds-xero-icon" src="https://edge.xero.com/images/1.0.0/logo/xero-logo-100.png" />
			<span class="ds-nav--header-title xui-padding">{{@root.options.title}}</span>
    </li>
		<li class="ds-nav--title xui-padding-vertical-large">
			<a class="ds-nav--link xui-padding-horizontal-large xui-section-title xui-verticaltab xui-padding-vertical-small {{#isEqual header @root.sections.0.header}} xui-verticaltab-is-selected{{/isEqual}}" href="./index.html">
					Overview
			</a>
		</li>
		{{#each menu}}
		{{! close previous section }}
		{{#if heading}}{{#unless @first}}</li></ul>{{/unless}}
			{{! section headings }}
    <li class="ds-nav--title xui-padding-vertical">
			<label class="ds-nav--link xui-section-title xui-padding-horizontal-large xui-padding-vertical-small xui-verticaltab" for="ds-input-{{referenceURI}}">
				{{header}}
			</label>
			<input id="ds-input-{{referenceURI}}" type="checkbox" class="ds-input" checked data-persistedmenu/>
			<svg class="ds-nav--icon xui-icon xui-icon-rotate-270 xui-button-icon-inverted"> <use xlink:href="#xui-icon-arrow" role="presentation"/></svg>

		{{else}}
			{{! else li for the leaf nodes }}
		<li class="xui-verticaltab {{#isEqual header @root.sections.0.header}} xui-verticaltab-is-selected{{/isEqual}}">
			<a class="ds-nav__menu-link ds-nav--link xui-padding-horizontal-large xui-padding-vertical-small" href="section-{{referenceURI}}.html"> {{header}} </a>
		{{/if}}

		{{#if heading}}<ul class="ds-nav--submenu xui-tabgroup xui-tabgroup-vertical xui-tabgroup-vertical-layout">{{else}}</li>{{/if}}
		{{/each}}
	</ul>
</nav>

<div class = "ds-footer xui-padding" aria-label = "Default example settings">
	<span class="xui-text-status xui-padding">
		<svg class="xui-icon xui-icon-inline">
			<use xlink:href="#xui-icon-view" role="presentation"/>
		</svg>
		<span class="xui-padding-horizontal-small">Set Default View:</span>
	</span>

	<button class="xui-button ds-defaultexample-label xui-padding-horizontal" onclick="changeExampleView(this)" data-statetype="markup">Markup</button>
	<button class="xui-button ds-defaultexample-label xui-padding-horizontal" onclick="changeExampleView(this)" data-statetype="example">Demo</button>
	<button class="xui-button ds-defaultexample-label xui-padding-horizontal" onclick="changeExampleView(this)" data-statetype="all">Both</button>
</div>

<!-- SCRIPTS -->
{{{scripts}}}
</body>

</html>
