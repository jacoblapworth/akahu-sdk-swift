{
  "name": "@xero/xui",
  "version": "11.0.0-alpha.1",
  "description": "Shared styles for Xero applications",
  "repository": {
    "type": "git",
    "url": "https://github.dev.xero.com/UXE/xui"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=6.9.0"
  },
  "files": [
    "react",
    "sass"
  ],
  "scripts": {
    "prepublish": "npm-run-all check:security check:engines",
    "check:security": "nsp check",
    "check:engines": "check-engines",
    "build": "grunt build",
    "build:prepublish": "npm-run-all build:babel copy:sass",
    "build:babel": "cross-env NODE_ENV=production babel src/react/ --out-dir react --copy-files --source-maps --ignore **/uitest,**/__tests__,**/docs",
    "build:umd": "npm-run-all clean:umd build:umd:entry build:umd:webpack",
    "build:umd:webpack": "cross-env NODE_ENV=production webpack --config webpack.umd.config.js",
    "build:umd:entry": "node umd/createEntryPoint.js",
    "clean": "npm-run-all clean:babel clean:umd clean:testui",
    "clean:babel": "rimraf react sass",
    "clean:umd": "rimraf src/react/umd.js umd/assets",
    "clean:testui": "rimraf uitest/assets",
    "copy:sass": "ncp src/sass sass",
    "dist": "grunt dist",
    "doc": "grunt doc",
    "lint": "npm-run-all lint:js lint:sass",
    "lint:sass": "stylelint --syntax scss ./src/sass/**/*.scss",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "watch": "grunt watch --livereload",
    "bump-major": "bumper major \"Bump to {version}\" --tag \"{version}\"",
    "bump-minor": "bumper minor \"Bump to {version}\" --tag \"{version}\"",
    "bump-patch": "bumper patch \"Bump to {version}\" --tag \"{version}\"",
    "update-readme": "grunt if:readme",
    "tokens": "grunt postcss",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.js --env=jsdom --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --config ./jest.config.js --env=jsdom --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --config ./jest.config.js --env=jsdom --no-cache --coverage",
    "test:ci": "cross-env TEAMCITY_VERSION=\"9.1.7\" NODE_ENV=test jest --config ./jest.config.js --env=jsdom --no-cache --coverage",
    "test:ui": "cross-env NODE_ENV=development webpack --watch --config ./webpack.testui.config.js"
  },
  "dependencies": {
    "@xero/xui-icon": "5.0.0-rc.2",
    "classnames": "2.1.0",
    "guid": "0.0.12",
    "lodash.debounce": "4.0.8",
    "lodash.throttle": "4.1.1",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-portal": "3.1.0",
    "verge": "1.10.2"
  },
  "devDependencies": {
    "@xero/babel-preset-xero": "1.0.2",
    "@xero/browserslist-autoprefixer": "2.3.0",
    "@xero/eslint-config-xero-react-base": "1.1.2",
    "autoprefixer": "7.1.1",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-jest": "20.0.3",
    "chai": "4.0.1",
    "check-engines": "1.2.0",
    "cross-env": "5.0.0",
    "css-loader": "0.28.4",
    "doiuse": "2.3.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "grunt": "0.4.5",
    "grunt-autoprefixer": "3.0.3",
    "grunt-babel": "6.0.0",
    "grunt-bower-install-simple": "1.1.4",
    "grunt-cli": "0.1.13",
    "grunt-contrib-clean": "0.6.0",
    "grunt-contrib-copy": "0.8.0",
    "grunt-contrib-csslint": "0.5.0",
    "grunt-contrib-cssmin": "0.13.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-git": "0.3.5",
    "grunt-if": "0.1.5",
    "grunt-postcss": "0.8.0",
    "grunt-sass": "1.1.0",
    "grunt-scss-lint": "0.3.8",
    "grunt-shell": "1.1.2",
    "grunt-template": "0.2.3",
    "jest": "20.0.4",
    "jest-teamcity-reporter": "0.6.2",
    "kss": "3.0.0-beta.16",
    "load-grunt-config": "0.17.2",
    "load-grunt-tasks": "3.2.0",
    "marked": "0.3.6",
    "ncp": "2.0.0",
    "node-sass": "3.13.1",
    "nopt": "3.0.6",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.3",
    "postcss": "5.2.4",
    "postcss-discard-comments": "2.0.4",
    "postcss-easy-import": "1.0.1",
    "postcss-loader": "2.0.5",
    "postcss-scss": "0.3.1",
    "react-addons-test-utils": "15.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "scrollbar-width": "3.1.1",
    "simple-mock": "0.8.0",
    "sinon": "2.3.2",
    "static-search-indexer": "git://github.dev.xero.com/XeroJS/static-search-indexer.git#1.0.0-alpha.2",
    "style-loader": "0.18.2",
    "stylelint": "7.7.1",
    "stylelint-order": "0.4.4",
    "uppercamelcase": "3.0.0",
    "webpack": "2.6.1",
    "yargs": "3.29.0"
  }
}
