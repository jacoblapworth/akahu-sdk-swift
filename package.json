{
  "version": "4.11.2",
  "name": "xui-icon",
  "main": "src/XUIIcon.js",
  "description": "Module for using XUI Icons",
  "author": "Kirk Holloway <kirk.holloway@xero.com>",
  "license": "Copyright Xero Ltd",
  "private": true,
  "scripts": {
    "build": "env UV_THREADPOOL_SIZE=100 xui-webpack --config ./conf/build.js --progress --colors && npm run rollup",
    "rollup": "rollup -c && rollup -c ./rollup.config.docs.js && rollup ./src/iconData.js --output ./dist/iconData.js --format cjs && node ./build/build.js",
    "prepublish": "bower install",
    "test": "env UV_THREADPOOL_SIZE=100 xui-karma start ./conf/test.js",
    "test-coverage": "env UV_THREADPOOL_SIZE=100 xui-karma start ./conf/test.js --coverage",
    "test-ui": "env UV_THREADPOOL_SIZE=100 xui-webpack --config ./conf/build.js --progress --colors --watch",
    "watch": "env UV_THREADPOOL_SIZE=100 xui-webpack --config ./conf/build.js --progress --colors --watch"
  },
  "devDependencies": {
    "babel-preset-react": "^6.5.0",
    "bower": "^1.7.0",
    "json-loader": "^0.5.4",
    "react": "^15.0.0",
    "react-dom": "^15.0.1",
    "rollup": "~0.36.0",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-npm": "^1.4.0",
    "rollup-plugin-uglify": "^0.3.1",
    "xui-webpack-build-tools": "git://github.dev.xero.com/ReactLabs/xui-webpack-build-tools.git#6.3.0"
  }
}
