{
  "version": "4.0.0",
  "name": "xui-switch",
  "main": "src/XUISwitch.js",
  "description": "Layout component for XUI Switches",
  "author": "Tim Redmond <tim.redmond@xero.com>",
  "license": "Copyright Xero Ltd",
  "private": true,
  "scripts": {
    "lint": "./node_modules/xui-webpack-build-tools/node_modules/.bin/eslint -c .eslintrc ./src",
    "prepublish": "bower install",
    "test-ui": "env UV_THREADPOOL_SIZE=100 ./node_modules/xui-webpack-build-tools/node_modules/.bin/webpack --config conf/build.js --progress --colors",
    "watch": "env UV_THREADPOOL_SIZE=100 ./node_modules/xui-webpack-build-tools/node_modules/.bin/webpack --config conf/build.js --progress --colors --watch",
    "test": "env UV_THREADPOOL_SIZE=100 ./node_modules/xui-webpack-build-tools/node_modules/.bin/karma start conf/test.js",
    "start": "./node_modules/serve/bin/serve test/ui"
  },
  "devDependencies": {
    "bower": "^1.6.3",
    "serve": "^1.4.0",
    "xui-webpack-build-tools": "git://github.dev.xero.com/ReactLabs/xui-webpack-build-tools.git"
  }
}
