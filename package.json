{
  "name": "@xero/xui",
  "version": "11.0.0-rc.5",
  "description": "Shared styles for Xero applications",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.dev.xero.com/UXE/xui"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3"
  },
  "files": [
    "react",
    "sass"
  ],
  "scripts": {
    "prepublish": "npm-run-all check:security check:engines",
    "check:security": "nsp check",
    "check:engines": "check-engines",
    "build": "grunt build",
    "build:prepublish": "npm-run-all clean:babel build:babel copy:sass",
    "build:babel": "cross-env NODE_ENV=production babel src/react/ --out-dir react --copy-files --source-maps --ignore **/uitest,**/__tests__,**/docs",
    "build:umd": "npm-run-all clean:umd build:umd:entry build:umd:webpack clean:umd:post-build",
    "build:umd:entry": "node umd/createEntryPoint.js",
    "build:umd:webpack": "cross-env NODE_ENV=production webpack --config webpack.umd.config.js",
    "clean": "npm-run-all clean:babel clean:umd clean:testui",
    "clean:babel": "rimraf react sass",
    "clean:umd": "rimraf src/react/umd.js umd/assets",
    "clean:umd:post-build": "rimraf src/react/umd.js",
    "clean:testui": "rimraf uitest/assets",
    "copy:sass": "ncp src/sass sass",
    "dist": "grunt dist",
    "doc": "cross-env NODE_ENV=production grunt doc",
    "lint": "npm-run-all lint:js lint:sass",
    "lint:sass": "stylelint --syntax scss ./src/sass/**/*.scss",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "watch": "cross-env NODE_ENV=development grunt watch --livereload",
    "bump-major": "bumper major \"Bump to {version}\" --tag \"{version}\"",
    "bump-minor": "bumper minor \"Bump to {version}\" --tag \"{version}\"",
    "bump-patch": "bumper patch \"Bump to {version}\" --tag \"{version}\"",
    "update-readme": "grunt if:readme",
    "tokens": "grunt postcss",
    "styleguide": "cross-env NODE_ENV=development styleguidist server --config=styleguide/config.js",
    "styleguide:build": "cross-env NODE_ENV=production styleguidist build --config=styleguide/config.js",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.js --env=jsdom --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --config ./jest.config.js --env=jsdom --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --config ./jest.config.js --env=jsdom --no-cache --coverage",
    "test:ci": "cross-env TEAMCITY_VERSION=\"9.1.7\" NODE_ENV=test jest --config ./jest.config.js --env=jsdom --no-cache --coverage",
    "test:ui": "cross-env NODE_ENV=development webpack --watch --config ./webpack.testui.config.js"
  },
  "dependencies": {
    "@xero/xui-icon": "5.0.0-rc.2",
    "classnames": "2.2.5",
    "lodash.debounce": "4.0.8",
    "lodash.throttle": "4.1.1",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-day-picker": "6.1.0",
    "react-dom": "15.6.1",
    "react-portal": "3.1.0",
    "scrollbar-width": "3.1.1",
    "uuid": "3.1.0",
    "verge": "1.10.2"
  },
  "devDependencies": {
    "@xero/babel-preset-xero": "1.3.2",
    "@xero/browserslist-autoprefixer": "2.3.1",
    "@xero/eslint-config-xero-react-base": "1.1.3",
    "array.prototype.find": "2.0.4",
    "autoprefixer": "7.1.2",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-jest": "20.0.3",
    "chai": "4.1.0",
    "check-engines": "1.5.0",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "doiuse": "3.0.0",
    "enzyme": "2.9.1",
    "eslint": "4.2.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "grunt": "1.0.1",
    "grunt-autoprefixer": "3.0.4",
    "grunt-bower-install-simple": "1.2.4",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-csslint": "2.0.0",
    "grunt-contrib-cssmin": "2.2.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-git": "1.0.4",
    "grunt-if": "0.1.5",
    "grunt-postcss": "0.8.0",
    "grunt-sass": "2.0.0",
    "grunt-scss-lint": "0.5.0",
    "grunt-shell": "2.1.0",
    "grunt-template": "1.0.0",
    "jest": "20.0.4",
    "jest-teamcity-reporter": "0.6.2",
    "kss": "3.0.0-beta.18",
    "load-grunt-config": "0.19.2",
    "load-grunt-tasks": "3.5.2",
    "marked": "0.3.6",
    "ncp": "2.0.0",
    "node-sass": "4.5.3",
    "nopt": "4.0.1",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.3",
    "postcss": "6.0.7",
    "postcss-discard-comments": "2.0.4",
    "postcss-easy-import": "2.1.0",
    "postcss-loader": "2.0.6",
    "postcss-scss": "1.0.2",
    "react-styleguidist": "5.5.7",
    "react-test-renderer": "15.6.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "simple-mock": "0.8.0",
    "sinon": "2.3.8",
    "style-loader": "0.18.2",
    "stylelint": "8.0.0",
    "stylelint-order": "0.6.0",
    "uppercamelcase": "3.0.0",
    "webpack": "3.3.0",
    "yargs": "8.0.2"
  }
}
